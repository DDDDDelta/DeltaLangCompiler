import cstdio;

let c u8 const = 'x';

let const ii = 42;

fn fib(i i32) i32 {
    if i == 0 {
        return 0;
    }
    else if i == 1 {
        return 1;
    }
    else {
        return fib(i - 2) + fib(i - 1);
    }
}

fn main() i32 {
    printf("hello world");
    printf("%d", ii);

    return fib(5);
}
